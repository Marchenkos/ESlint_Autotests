{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true
    },
    "extends": [
        "airbnb"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "no-console":"off",
        "no-plusplus":"off",
        "no-unused-vars":"warn",
        "max-classes-per-file":"off", 
        "prefer-rest-params":"warn", 
        "eqeqeq":"warn",
        "no-restricted-syntax": "off"
    }
}